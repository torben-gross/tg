#version 450
#extension GL_ARB_separate_shader_objects : enable

layout(local_size_x = 1) in;

layout(set = 0, binding = 0) buffer se_buffer0
{
	float se_array0[10];
};

layout(set = 0, binding = 1) buffer se_buffer1
{
	float se_array1[10];
};

void main()
{
	uint i = gl_GlobalInvocationID.x;
	se_array1[i] = se_array0[i];
	se_array0[i] = 33.0f;
}
